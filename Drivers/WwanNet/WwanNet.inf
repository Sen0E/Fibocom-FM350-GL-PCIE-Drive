;------------------------------------------------------------------------------
; WDF specific section
;------------------------------------------------------------------------------
[WwanNet_Device.NT.Wdf]
KmdfService                 = %WwanNet.ServiceName%, WwanNet_Install
UmdfService                 = WwanMControl, WwanMControl_Install
UmdfServiceOrder            = WwanMControl
UmdfKernelModeClientPolicy  = AllowKernelModeClients
UmdfHostProcessSharing      = ProcessSharingDisabled
UmdfFileObjectPolicy        = AllowNullAndUnknownFileObjects
UmdfImpersonationLevel      = Impersonation
;UmdfMethodNeitherAction     = Copy
;UmdfDispatcher              = FileHandle

[WwanMControl_Install]
UmdfLibraryVersion         = 2.15.0
ServiceBinary              = %12%\UMDF\WwanIHVDriver.dll

[WwanNet_Install]
KmdfLibraryVersion = 1.27

;[WwanNet_Device.NT.CoInstallers]
;AddReg=CoInstallers_AddReg
;[CoInstallers_AddReg]
;HKR,,CoInstallers32,0x00010000,"WUDFCoinstaller.dll"


;------------------------------------------------------------------------------
; Service installation
;------------------------------------------------------------------------------
[WwanNet_Device.NT.Services]
AddService = WUDFRd,0x000001f8,WUDFRD_ServiceInstall
AddService = %WwanNet.ServiceName%, %SPSVCINST_ASSOCSERVICE%, WwanNet_Service_Inst
AddService = %MA.SrvName%, 0x00000800, MAService_Inst

[WUDFRD_ServiceInstall]
DisplayName     = %WudfRdDisplayName%
ServiceType     = 1
StartType       = 3
ErrorControl    = 1
ServiceBinary   = %12%\WUDFRd.sys

[MAService_Inst]
DisplayName   = %MA.SrvName%
Description   = %MA.Description%
ServiceType   = 0x00000010
StartType     = 2
ErrorControl  = 1
ServiceBinary = %11%\IntelWWANModemAuthenticator.exe

[WwanNet_Service_Inst]
DisplayName    = %WwanNet.DisplayName%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\WwanNet.sys
LoadOrderGroup = Extended Base
;AddReg        = WwanNet_Service_Inst.Reg

;[WwanNet_Service_Inst.Reg]
;HKR,,	BootFlags, 0x00010001, 1 ; CM_SERVICE_NETWORK_BOOT_LOAD
;[Service.EventLog]
;AddReg                  = Service.AddEventLog.reg
;[Service.AddEventLog.reg]
;HKR,, EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
;HKR,, TypesSupported,   0x00010001, 7
;


