;------------------------------------------------------------------------------
; WwanNet.INF
;
; Intel(R) Wwan 5G NetAdapter Stack
; Installation Script for Windows 64 bit
;
;------------------------------------------------------------------------------



;------------------------------------------------------------------------------
; Version Section
;------------------------------------------------------------------------------
[Version]
Signature               = "$WINDOWS NT$"
Class                   = Net
ClassGUID               = {4d36e972-e325-11ce-bfc1-08002be10318}
PnpLockDown             = 1
Provider                = %ManufacturerName%
CatalogFile             = WwanNet.cat
DriverVer =06/16/2022,0.6.200.233

;------------------------------------------------------------------------------
; Manufacturer
;------------------------------------------------------------------------------
[Manufacturer]
%ManufacturerName%=Standard,NTamd64

;------------------------------------------------------------------------------
;Device
;------------------------------------------------------------------------------
[Standard.NTamd64]
%WwanNet.DeviceDesc%=WwanNet_Device, PCI\VEN_14C3&DEV_4D75
%WwanNet.DeviceDesc%=WwanNet_Device, PCI\VEN_14C3&DEV_4D75&SUBSYS_35021CF8
%WwanNet.DeviceDesc%=WwanNet_Device, PCI\VEN_14C3&DEV_4D75&SUBSYS_35051CF8

;------------------------------------------------------------------------------
;INSTALL WINT_64
;------------------------------------------------------------------------------
[WwanNet_Device.NT]
CopyFiles=Drivers_Dir,Drivers_Dir_Umdf,Service_Dir
CopyINF=WwanComDriver.inf
;
FeatureScore            = 0xFE
Characteristics         = 0x04  ; NCF_PHYSICAL
BusType                 = 5     ; PCIBus
*IfType                 = 243   ; IF_TYPE_WWANPP
*MediaType              = 9     ; NdisMediumWirelessWan
*PhysicalMediaType      = 8     ; NdisPhysicalMediumWirelessWan
*IfConnectorPresent     = 1     ; true
*ConnectionType         = 1     ; NET_IF_CONNECTION_DEDICATED
*DirectionType          = 0     ; NET_IF_DIRECTION_SENDRECEIVE
*AccessType             = 2     ; NET_IF_ACCESS_BROADCAST
*HardwareLoopback       = 0     ; false
NumberOfNetworkInterfaces = 11  ; how many network interfaces the NIC supports
EnableDhcp              = 0;    ; Disable DHCP -  does not implement DHCP server spoofing
;
AddReg                  = ndi.reg
AddReg                  = wwanfeatures.staticparams.reg

;------------------------------------------------------------------------------
; AddReg section for each Advanced Keyword
; These leys go into 'The device's software key'
;------------------------------------------------------------------------------
;
AddReg                  = SelectiveSuspend.kw ; Only needed for 19042 OS. For later OS remove!
AddReg                  = DeviceSleepOnDisconnect.kw
AddReg                  = WakeOnMagicPacket.kw
;AddReg                  = ModernStandyWoLMagicPacket.kw

;
;AddReg                  = OffloadChecksum.kw

;
;
;AddReg                  = RSS.kw
;AddReg                  = LSO.kw
;AddReg                  = USO.kw


;------------------------------------------------------------------------------
; Device feature registry entries
; Format: reg-root, [subkey],[value-entry-name],[flags],[value][,[value]]
;------------------------------------------------------------------------------
[wwanfeatures.staticparams.reg]
;
HKR,, WWANCIDBufferPortConfig, %RegTypeDW%, 0
HKR,, WWANCIDBufferTRACE,      %RegTypeDW%, 1
HKR,, WWANCDConfig,            %RegTypeDW%, 0
HKR,, WWANMiniConfig,          %RegTypeDW%, 0
;
HKR,, WwanMbimVersionDowngrade, %RegTypeDW%, 0

;
; These need to be removed eventually !!!
HKR,, WWANFeatureGNSS,    %RegTypeDW%, 1
HKR,, WWANFeatureAT,      %RegTypeDW%, 1
HKR,, WWANFeatureCD,      %RegTypeDW%, 0
HKR,, WWANFeatureMDLOG,   %RegTypeDW%, 0
HKR,, WWANFeatureSAPLOG,  %RegTypeDW%, 0
HKR,, WWANFeatureADB,     %RegTypeDW%, 0
HKR,, WWANFeatureMETASAP, %RegTypeDW%, 0
HKR,, WWANFeatureMETAMD,  %RegTypeDW%, 0
HKR,, WWANFeatureNPT,     %RegTypeDW%, 0
HKR,, WWANFeatureFLASH,   %RegTypeDW%, 0
HKR,, WWANFeatureNMEA,    %RegTypeDW%, 0
;

;------------------------------------------------------------------------------
; Binding
; Note that "ndis5" simply means to bind to TCPIP; it is not a version number
;------------------------------------------------------------------------------
[ndi.reg]
HKR, Ndi\Interfaces,    UpperRange, 0, "flpp4, flpp6"
HKR, Ndi\Interfaces,    LowerRange, 0, "ppip"
HKR, Ndi,               Service,    0, %WwanNet.ServiceName%

